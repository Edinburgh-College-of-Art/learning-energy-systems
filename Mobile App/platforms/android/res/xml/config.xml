<?xml version='1.0' encoding='utf-8'?>
<widget id="com.coolappz.energy_diary" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="YoikScreenOrientation">
        <param name="android-package" value="net.yoik.cordova.plugins.screenorientation.YoikScreenOrientation" />
    </feature>
    <name>energy_diary</name>
    <description>
        Learning Energy Systems Project description. University of Edinburgh
    </description>
    <author email="h.mehrpouya@ed.ac.uk" href="http://hadi.link">
        Learning Energy Systems Project. University of Edinburgh
    </author>
    <content src="index.html" />
    <access origin="*" />
    <preference name="Fullscreen" value="true" />
</widget>
